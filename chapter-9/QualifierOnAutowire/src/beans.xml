<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
		http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context 
		http://www.springframework.org/schema/context/spring-context-3.2.xsd">

	<context:annotation-config />
	<context:component-scan base-package="org.packt.Spring.chapter3.Annotation.qualifier" />


	<bean id="employeeA" class="org.packt.Spring.chapter3.Annotation.qualifier.Employee">
		<property name="str" value="employee-A"></property>
	</bean>


	<bean id="employeeB" class="org.packt.Spring.chapter3.Annotation.qualifier.Employee">
		<property name="str" value="employee-B"></property>
	</bean>

	<bean id="employeeC" class="org.packt.Spring.chapter3.Annotation.qualifier.Employee">
		<property name="str" value="employee-C"></property>
	</bean>

</beans>
